#include <iostream>
using namespace std;

int main() 
{
int age=0;
cout << "Enter the age of a User"<< endl;
cin>>age;

if (age>=18)
{cout<<"You are eligible for Voting!\n"<<endl;}
else
{age=18-age;
cout<<"You are not eligible for Voting\n"<<"You need to wait for "<<age<<" year(s) "<<endl;}
return 0;
}
